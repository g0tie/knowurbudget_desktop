  "build": {
    "appId": "com.loyno-mathcs.g0tie.knowurbudget",
    "files": [ "build/**/*", "node_modules/**/*" ],
    "directories":{
  "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
